{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "https://spotify-explorer-backend.onrender.com/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install"
}
